<?xml version="1.0" encoding="utf-8" ?>
<pam_mount>

<volume 
    user="*" 
    not="ubuntu"
    fstype="cifs" 
    server="10.0.0.7" 
    path="partages" 
    mountpoint="/home/%(USER)/Partage" 
    options="username=%(USER),domain=PROTO,uid=%(USERUID),gid=%(USERGID),dir_mode=0755,file_mode=0644" 
/>

<!-- Dossier personnel (home) -->
<volume 
    user="*" 
    not="ubuntu"
    fstype="cifs" 
    server="10.0.0.7" 
    path="%(USER)" 
    mountpoint="/home/%(USER)/Personnel" 
    options="username=%(USER),domain=PROTO,uid=%(USERUID),gid=%(USERGID),dir_mode=0700,file_mode=0600" 
/>

<mntoptions require="nosuid,nodev" />
<logout wait="0" hup="0" term="no" kill="no" />
<mkmountpoint enable="1" remove="true" />

</pam_mount>
